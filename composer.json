{
    "name": "p-ba/symfony-serializer-ext",
    "description": "This package extends standard Symfony Serializer functionality",
    "keywords": [
        "serialization",
        "deserialization",
        "json",
        "xml"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {"PBA\\": ["src/", "tests/"]}
    },
    "require": {
        "php": "^7.2",
        "symfony/serializer": "^4.1",
        "symfony/property-access": "^4.1",
        "symfony/property-info": "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Baraulya",
            "email": "pbaraulya@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
